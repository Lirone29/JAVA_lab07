<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="489.0" prefWidth="716.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dataBaseProject.doctorController">
   <children>
      <Button fx:id="showTodayAppointment" layoutX="512.0" layoutY="108.0" mnemonicParsing="false" onAction="#showTodayAppointment" prefHeight="40.0" prefWidth="128.0" text="Show Appointment" />
      <TableView fx:id="infoTable" layoutX="14.0" layoutY="41.0" prefHeight="240.0" prefWidth="448.0">
        <columns>
          <TableColumn fx:id="patientIDColumn" prefWidth="75.0" text="Patient ID" />
            <TableColumn fx:id="nameColumn" prefWidth="75.0" text="Name" />
            <TableColumn fx:id="surnameColumn" prefWidth="75.0" text="Surname" />
          <TableColumn fx:id="timeColumn" prefWidth="75.0" text="Time" />
            <TableColumn fx:id="statusColumn" prefWidth="75.0" text="Status" />
            <TableColumn fx:id="resumeColumn" prefWidth="75.0" text="Resume" />
        </columns>
      </TableView>
      <Button fx:id="lastVisitButton" layoutX="512.0" layoutY="178.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="128.0" text="Last Visit" />
