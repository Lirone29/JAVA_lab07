      <TextField fx:id="dateTextField" layoutX="497.0" layoutY="50.0" prefHeight="25.0" prefWidth="159.0" promptText="Enter Date (YYYY-MM-DD)" />
      <Button fx:id="statusButton" layoutX="272.0" layoutY="308.0" mnemonicParsing="false" onAction="#changeStatus" prefHeight="40.0" prefWidth="128.0" text="Change Status" />
      <TextField fx:id="resumeTextField" layoutX="14.0" layoutY="396.0" promptText="Resume" />
      <TextField fx:id="statusTextField" layoutX="14.0" layoutY="315.0" promptText="Status" />
   </children>
</AnchorPane>
